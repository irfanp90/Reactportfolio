  "bugs": {
    "url": "https://github.com/irfanp90/Reactportfolio/issues"
  },
  "homepage": "https://github.com/irfanp90/Reactportfolio#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "hover.css": "^2.3.2",
    "if-env": "^1.0.4",
    "mongoose": "^5.5.8",
    "react-particles-js": "^2.6.0",
    "react-router-dom": "^5.0.0",
    "reactstrap": "^8.0.0"
  }
}
